<%- include ("./partials/head") %>
    <h1>News</h1>
    <section id="news">
        <% newsData.forEach(news=> { %>
            <figure>
                <h2>
                    <%= news.title %>
                </h2>
                <h6>
                    <%= news.publishedAt %>
                </h6>
                <img src="<%= news.urlToImage %>" alt="">
                <figcaption>
                    <p>
                        <%= news.description %>
                    </p>
                    <span>
                        <%= news.author %>
                    </span>
                    <span>
                        <a href="<%= news.content %>">Click4More</a>
                    </span>

                </figcaption>
            </figure>
            <% }) %>
    </section>
    </body>

    </html>